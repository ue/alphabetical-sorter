{
  "name": "alphabetical-sorter",
  "displayName": "Alphabetical Sorter",
  "description": "Multi line or single line alphabetical sorter for JS",
  "version": "2.0.1",
  "icon": "images/icon.png",
  "publisher": "ue",
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:extension.sortIt"],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sortIt",
        "title": "Alphabetical Sorter"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "eslint": "^4.11.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
